{
  "name": "sunstealer.explorer",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --open --host 0.0.0.0 --port 8080 --disable-host-check",
    "build": "run-script-os",
    "build:linux": "npm run clean && ng build --source-map && tsc ./src/server/index.ts && cp ./src/server/*.js ./dist",
    "build:win32": "npm run clean && ng build --source-map && tsc ./src/server/index.ts && copy .\\src\\server\\*.js .\\dist",
    "clean": "run-script-os",
    "clean:linux": "rm -rf dist/* && rm src/server/index.js || true",
    "clean:win32": "rmdir .\\dist /s /q",
    "start": "node ./dist/index.js",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.5",
    "@angular/material-moment-adapter": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "moment": "^2.29.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@ibm/plex": "^6.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/web-push": "^3.3.2",
    "base64-js": "^1.5.1",
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.5",
    "process": "^0.11.10",
    "run-script-os": "^1.1.6",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.5.0",
    "typescript": "~4.8.0",
    "util": "^0.12.5",
    "web-push": "^3.5.0"
  }
}
