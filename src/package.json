{
  "name": "sunstealer-explorer-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --open --host 0.0.0.0 --port 8080 --disable-host-check",
    "build": "run-script-os",
    "build:linux": "npm run clean && ng build --source-map && tsc ./src/server/index.ts && cp ./src/server/*.js ./dist",
    "build:win32": "npm run clean && ng build --source-map && tsc ./src/server/index.ts && copy .\\src\\server\\*.js .\\dist",
    "clean": "run-script-os",
    "clean:linux": "rm -rf dist/* && rm src/server/index.js || true",
    "clean:win32": "rmdir .\\dist /s /q",
    "start": "node ./dist/index.js",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^18.1.1",
    "@ngrx/router-store": "^18.1.1",
    "@ngrx/store": "^18.1.1",
    "@ngrx/store-devtools": "^18.1.1",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@ibm/plex": "^6.4.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.3",
    "@types/web-push": "^3.6.4",
    "base64-js": "^1.5.1",
    "body-parser": "^1.20.3",
    "express": "^4.21.1",
    "run-script-os": "^1.1.6",
    "tslib": "^2.8.1",
    "typescript": "~5.5.2",
    "util": "^0.12.5",
    "web-push": "^3.6.7"
  }
}
